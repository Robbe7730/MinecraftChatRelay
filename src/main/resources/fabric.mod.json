{
  "schemaVersion": 1,
  "id": "minecraft-chat-relay",

  "name": "Minecraft Chat Relay",
  "description": "Fabric plugin for relaying between different chats (including Minecraft in-game chat) ",
  "authors": [
    "Robbe Van Herck"
  ],
  "contact": {
    "email": "mincraftchatrelay@robbevanherck.be",
    "sources": "https://github.com/Robbe7730/MinecraftChatRelay",
    "issues": "https://github.com/Robbe7730/MinecraftChatRelay/issues"

  },

  "license": "MIT",
  "icon": "assets/chatplugin/icon.png",

  "environment": "server",
  "entrypoints": {
    "main": [
      "be.robbevanherck.chatplugin.ChatPlugin"
    ]
  },
  "mixins": [
    "chatplugin.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.5"
  },
  "suggests": {
    "flamingo": "*"
  },
  "version": "$version"
}
